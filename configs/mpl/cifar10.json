{
    "model": {
        "backbone": "wide_resnet28_2",
        "num_classes": 10,
        "batch_norm_decay": 0.99,
        "label_smoothing": {
            "student": 0.1,
            "teacher": 0.1
        },
        "dropout_rate": {
            "student": 0.35, 
            "teacher": 0.5
        },
        "UDA_factor": 1.0,
        "UDA_temperatrue": 0.8
    },
    "dataset": {
        "name": "cifar10",
        "root": "data/cifar10",
        "num_labeled": 4000,
        "batch_size": {
            "labeled": 128,
            "unlabeled": 128
        }
    },
    "optimizer": {
        "student": {
            "name": "SGD",
            "lr": 0.3,
            "momentum": 0.9,
            "weight_decay": 0.0005,
            "nesterov": false
        },
        "teacher": {
            "name": "SGD",
            "lr": 0.125,
            "momentum": 0.9,
            "weight_decay": 0.0005,
            "nesterov": false
        }
    },
    "scheduler": {
        "student": {
            "name": "CosineAnnealingWithWarmUp",
            "interval": "step",
            "T_max": 1000000,
            "T_warmup": 2000
        },
        "teacher": {
            "name": "CosineAnnealingWithWarmUp",
            "interval": "step",
            "T_max": 1000000,
            "T_warmup": 2000
        }
    },
    "trainer": {
        "max_steps": 1000000
    }
}