{
    "model": {
        "backbone": "wide_resnet28_2",
        "num_classes": 10,
        "pretrained": false,
        "batch_norm_decay": 0.99,
        "label_smoothing": {
            "student": 0.2,
            "teacher": 0.2
        },
        "dropout_rate": {
            "student": 0.35, 
            "teacher": 0.5
        },
        "UDA": {
            "threshold": 0.8,
            "temperatrue": 0.8,
            "factor": 8.0,
            "warmup_step": 5000
        }
    },
    "dataset": {
        "name": "ssl_cifar10",
        "root": "data/cifar10",
        "num_labeled": 4000,
        "batch_size": {
            "labeled": 128,
            "unlabeled": 896
        }
    },
    "optim": {
        "student": {
            "optimizer": {
                "name": "SGD",
                "lr": 0.05,
                "momentum": 0.9,
                "weight_decay": 0.0005,
                "nesterov": true
            },
            "scheduler": {
                "name": "CosineAnnealingWarmUp",
                "T_max": 300000,
                "T_warm": 2000
            },
            "interval": "step"
        },
        "teacher": {
            "optimizer": {
                "name": "SGD",
                "lr": 0.05,
                "momentum": 0.9,
                "weight_decay": 0.0005,
                "nesterov": true
            },
            "scheduler":{
                "name": "CosineAnnealingWarmUp",
                "T_max": 300000,
                "T_warm": 5000
            },
            "interval": "step"
         }
    },
   "trainer": {
        "max_steps": 300000
    }
}