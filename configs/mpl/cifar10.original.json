{
    "model": {
        "label_smoothing": {
            "student": 0.1,
            "teacher": 0.1
        },
        "UDA": {
            "threshold": 0.6,
            "temperature": 0.8,
            "factor": 1.0,
            "warmup": 2000
        },
        "EMA": {
            "decay": 0.995
        }
    },
    "dataset": {
        "name": "ssl_cifar10",
        "root": "data/cifar10",
        "num_labeled": 4000,
        "batch_size": {
            "labeled": 128,
            "unlabeled": 128
        }
    },
    "optim": {
        "student": {
            "optimizer": {
                "name": "SGD",
                "lr": 0.3,
                "momentum": 0.9,
                "weight_decay": 0.0005,
                "nesterov": true
            },
            "scheduler": {
                "name": "CosineAnnealingWarmUp",
                "T_max": 1000000,
                "T_warm": 2000
            },
            "interval": "step"
        },
        "teacher": {
            "optimizer": {
                "name": "SGD",
                "lr": 0.125,
                "momentum": 0.9,
                "weight_decay": 0.0005,
                "nesterov": true
            },
            "scheduler":{
                "name": "CosineAnnealingWarmUp",
                "T_max": 1000000,
                "T_warm": 2000
            },
            "interval": "step"
         }
    },
   "trainer": {
        "max_steps": 1000000
    }
}